-- ----------------------------
-- Function structure for fn_anggaran
-- ----------------------------
DROP FUNCTION IF EXISTS "integrasi"."fn_anggaran"(int4);
CREATE OR REPLACE FUNCTION "integrasi"."fn_anggaran"(int4)
  RETURNS "pg_catalog"."text" AS $BODY$
DECLARE





        result  text;





BEGIN





      





        select into result sum(f_total_anggaran) as f_total_anggaran from integrasi.t_monitoring_anggaran where f_tahun = $1;











        RETURN result;











    END;
$BODY$
  LANGUAGE plpgsql VOLATILE
  COST 100;

-- ----------------------------
-- Function structure for fn_jenis
-- ----------------------------
DROP FUNCTION IF EXISTS "integrasi"."fn_jenis"(int4);
CREATE OR REPLACE FUNCTION "integrasi"."fn_jenis"(int4)
  RETURNS "pg_catalog"."text" AS $BODY$
DECLARE





        result  text;





BEGIN





      





        select into result f_nama from integrasi.t_monitoring_jenis where f_monitoring_jenis_id = $1;











        RETURN result;











    END;
$BODY$
  LANGUAGE plpgsql VOLATILE
  COST 100;

-- ----------------------------
-- Function structure for fn_randompassword
-- ----------------------------
DROP FUNCTION IF EXISTS "integrasi"."fn_randompassword"();
CREATE OR REPLACE FUNCTION "integrasi"."fn_randompassword"()
  RETURNS "pg_catalog"."int4" AS $BODY$
DECLARE
        result int4;
        valid boolean;
        bilangan int4;
BEGIN
        valid := false;
        
        LOOP
            select into bilangan substr(random()::text,4,6);
            if (bilangan > 0) AND (bilangan < 20000000) then 
                valid := TRUE;
            end if;  
            EXIT WHEN valid = TRUE;
        END LOOP; 

        result := bilangan;

        RETURN result;
END;
$BODY$
  LANGUAGE plpgsql VOLATILE
  COST 100;

