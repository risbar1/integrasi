-- ----------------------------
-- Table structure for k_jalurmasuk
-- ----------------------------
DROP TABLE IF EXISTS "integrasi"."k_jalurmasuk";
CREATE TABLE "integrasi"."k_jalurmasuk" (
  "f_kode" varchar(20) COLLATE "pg_catalog"."default" NOT NULL,
  "f_nama" varchar(255) COLLATE "pg_catalog"."default",
  "f_tgl_create" timestamp(6),
  "f_last_update" timestamp(6),
  "f_soft_delete" numeric(1,0),
  "f_username" varchar(20) COLLATE "pg_catalog"."default",
  "f_batasbayar_awal" date,
  "f_batasbayar_akhir" date,
  "f_jalurmasuk_id" int4 NOT NULL DEFAULT nextval(('integrasi.k_jalurmasuk_f_jalurmasuk_id_seq'::text)::regclass),
  "f_nomor_peserta_satu" varchar(255) COLLATE "pg_catalog"."default",
  "f_biaya_1" int8,
  "f_biaya_2" int8,
  "f_biaya_3" int8,
  "f_biaya_4" int8,
  "f_kuota_pendaftar" int8,
  "f_max_pilihan" int8,
  "f_nomor_peserta_dua" varchar(255) COLLATE "pg_catalog"."default",
  "f_referensi" varchar(20) COLLATE "pg_catalog"."default",
  "f_lintas" varchar(6) COLLATE "pg_catalog"."default",
  "f_raport" varchar(6) COLLATE "pg_catalog"."default",
  "f_kelompokstudi" varchar(6) COLLATE "pg_catalog"."default",
  "f_batas_finalisasi" date,
  "f_pembayaran" varchar(6) COLLATE "pg_catalog"."default",
  "f_tampil_depan" varchar(6) COLLATE "pg_catalog"."default",
  "f_kodebank" varchar(150) COLLATE "pg_catalog"."default"
)
;

