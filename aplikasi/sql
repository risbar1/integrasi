        select into result f_nama from integrasi.t_monitoring_user where f_monitoring_user_id = $1;











        RETURN result;











    END;
$BODY$
  LANGUAGE plpgsql VOLATILE
  COST 100;

-- ----------------------------
-- View structure for v_otoritasuser
-- ----------------------------
DROP VIEW IF EXISTS "integrasi"."v_otoritasuser";
CREATE VIEW "integrasi"."v_otoritasuser" AS  SELECT t_password.f_password_id,
    t_password.f_username,
    t_password.f_password,
    t_password.f_user_id,
    t_password.f_nama,
    t_password.f_statusaktif,
    t_otoritasuser.f_otoritas_id,
    t_otoritasuser.f_soft_delete AS otoritasuser_f_soft_delete,
    t_otoritas.f_otoritas,
    t_otoritas.f_otoritas_website,
    t_otoritas.fn_otoritas_website,
    t_otoritas.f_soft_delete
   FROM integrasi.t_password
     JOIN integrasi.t_otoritasuser ON t_password.f_password_id = t_otoritasuser.f_password_id
     JOIN integrasi.t_otoritas ON t_otoritas.f_otoritas_id = t_otoritasuser.f_otoritas_id;

